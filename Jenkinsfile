pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
               script{
                   echo "Compile the Code "
               }
            }
        }
        stage('UnitTest') {
            steps {
               script{
                   echo "Unit Test the Code "
               }
            }
        }
        stage('Package') {
            steps {
               script{
                   echo "Package the Code "
               }
            }
        }
    }
}

